{
    "name": "powerglitch",
    "version": "2.3.4",
    "repository": {
        "type": "git",
        "url": "https://github.com/7PH/json-cursor-path.git"
    },
    "type": "module",
    "main": "./lib/src/index.js",
    "types": "./lib/src/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build:docs": "vite build --emptyOutDir",
        "build:api-docs": "npx typedoc",
        "build:lib": "tsc",
        "build:dist": "rm -rf dist; rollup -c",
        "build": "npm run build:docs && npm run build:api-docs && npm run build:lib && npm run build:dist",
        "size": "size-limit",
        "lint": "eslint src/**/*.ts docs-src/**/*.vue",
        "lint-fix": "eslint --fix src/**/*.ts docs-src/**/*.vue",
        "test": "npm run lint && jest"
    },
    "files": [
        "/lib/src",
        "/dist"
    ],
    "devDependencies": {
        "@babel/core": "^7.24.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-typescript": "^7.24.7",
        "@fortawesome/fontawesome-svg-core": "^6.5.2",
        "@fortawesome/free-brands-svg-icons": "^6.5.2",
        "@fortawesome/free-regular-svg-icons": "^6.5.2",
        "@fortawesome/free-solid-svg-icons": "^6.5.2",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "@highlightjs/vue-plugin": "^2.1.2",
        "@size-limit/preset-small-lib": "^11.1.4",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "autoprefixer": "^10.4.19",
        "babel-jest": "^29.7.0",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.27.0",
        "highlight.js": "^11.10.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom-testing-mocks": "^1.13.0",
        "lodash": "^4.17.21",
        "pinia": "^2.1.7",
        "postcss": "^8.4.39",
        "rollup": "^4.18.0",
        "rollup-plugin-esbuild": "^6.1.1",
        "size-limit": "^11.1.4",
        "tailwindcss": "^3.4.4",
        "typedoc": "^0.26.3",
        "typescript": "^5.5.3",
        "vite": "^5.3.3",
        "vue": "^3.4.31",
        "vue-router": "^4.4.0"
    },
    "size-limit": [
        {
            "limit": "2.1 kB",
            "path": "dist/powerglitch.min.js"
        }
    ]
}
